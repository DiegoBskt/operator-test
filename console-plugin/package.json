{
    "name": "cluster-assessment-plugin",
    "version": "1.2.32",
    "private": true,
    "repository": "https://github.com/diegobskt/cluster-assessment-operator",
    "license": "Apache-2.0",
    "scripts": {
        "clean": "rm -rf dist",
        "build": "yarn clean && NODE_ENV=production yarn ts-node ./node_modules/.bin/webpack",
        "build:dev": "yarn clean && yarn ts-node ./node_modules/.bin/webpack",
        "start": "yarn ts-node ./node_modules/.bin/webpack serve",
        "start-console": "./start-console.sh",
        "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
        "lint": "eslint src --ext .ts,.tsx",
        "test": "jest"
    },
    "consolePlugin": {
        "name": "cluster-assessment-plugin",
        "version": "1.2.32",
        "displayName": "Cluster Assessment",
        "description": "View and manage cluster assessments with health scores and recommendations",
        "exposedModules": {
            "AssessmentDashboard": "./components/AssessmentDashboard",
            "AssessmentDetails": "./components/AssessmentDetails"
        },
        "dependencies": {
            "@console/pluginAPI": "*"
        }
    },
    "devDependencies": {
        "@openshift-console/dynamic-plugin-sdk": "4.20.0",
        "@openshift-console/dynamic-plugin-sdk-webpack": "4.20.0",
        "@patternfly/react-charts": "^7.4.0",
        "@patternfly/react-core": "^5.4.14",
        "@patternfly/react-icons": "^5.4.2",
        "@patternfly/react-table": "^5.4.14",
        "@types/node": "^18.19.0",
        "@types/react": "^17.0.80",
        "@types/react-dom": "^17.0.25",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "@typescript-eslint/parser": "^6.21.0",
        "copy-webpack-plugin": "^12.0.0",
        "css-loader": "^6.10.0",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-i18next": "^11.18.6",
        "react-router": "^5.3.4",
        "react-router-dom": "^5.3.4",
        "style-loader": "^3.3.4",
        "ts-loader": "^9.5.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3",
        "webpack": "^5.90.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1"
    },
    "resolutions": {
        "@types/react": "^17.0.80",
        "@types/react-dom": "^17.0.25"
    }
}